if(is.na(x){
x_out <- as.numeric(x)
}else{
x_out <-   x+rnorm(1, 0, 0.5)
if(x_out > max_val){
x_out <- max_val
}
if(x_out < min_val){
x_out <- min_val
}}
return(x_out)
}
